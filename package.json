{
  "name": "pjserver",
  "version": "1.0.0",
  "description": "プロセカ民営公園の専属Botです。",
  "private": true,
  "type": "module",
  "main": "./dist/master.js",
  "bin": {
    "pjserver": "./dist/master.js"
  },
  "scripts": {
    "build": "pnpm run copy-static && tsc -p tsconfig.json && tsc-alias",
    "build:prod": "pnpm run copy-static && tsc -p tsconfig.build.json --sourceMap false && tsc-alias",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "format": "prettier ./src --check",
    "format:fix": "prettier ./src --write",
    "copy-static": "node -e \"require('fs-extra').copySync('src','dist',{filter:f=>require('path').extname(f)!=='.ts'})\"",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AkatukiSora/pjserver-sys.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AkatukiSora/pjserver-sys/issues"
  },
  "homepage": "https://github.com/AkatukiSora/pjserver-sys#readme",
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.70",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "fs-extra": "^11.3.0",
    "log4js": "^6.9.1",
    "net": "^1.0.2",
    "node-fetch": "^3.3.2",
    "pureimage": "^0.4.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/eslint": "^9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.21",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.3.5",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "3.5.3",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
  "overrides": {}
}
