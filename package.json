{
  "name": "pjserver",
  "version": "1.0.0",
  "description": "プロセカ民営公園の専属Botです。",
  "private": true,
  "type": "module",
  "main": "./dist/master.js",
  "scripts": {
    "build": "node scripts/copy-assets.js && tsc -p tsconfig.build.json && tsc-alias",
    "build:prod": "node scripts/copy-assets.js && tsc -p tsconfig.build.json --sourceMap false && tsc-alias",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "format": "prettier ./**/*.ts --check",
    "format:fix": "prettier ./**/*.ts --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AkatukiSora/pjserver-sys.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AkatukiSora/pjserver-sys/issues"
  },
  "homepage": "https://github.com/AkatukiSora/pjserver-sys#readme",
  "dependencies": {
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.5",
    "log4js": "^6.9.1",
    "net": "^1.0.2",
    "node-fetch": "^3.3.2",
    "pureimage": "^0.4.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/eslint": "^8.56.10",
    "@types/fs-extra": "^11",
    "@types/node": "^20.12.8",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "fs-extra": "^11.3.0",
    "prettier": "3.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  },
  "packageManager": "yarn@4.9.1"
}
