{
  "name": "pjserver",
  "version": "1.0.0",
  "description": "プロセカ民営公園の専属Botです。",
  "private": true,
  "type": "module",
  "main": "./dist/master.js",
  "bin": {
    "pjserver": "./dist/master.js"
  },
  "scripts": {
    "build": "pnpm run copy-static && tsc -p tsconfig.json && tsc-alias",
    "build:prod": "pnpm run copy-static && tsc -p tsconfig.build.json && tsc-alias",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "format": "prettier ./src --check",
    "format:fix": "prettier ./src --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:coverage": "vitest run --coverage",
    "copy-static": "tsx scripts/copy-static.ts",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AkatukiSora/pjserver-sys.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AkatukiSora/pjserver-sys/issues"
  },
  "homepage": "https://github.com/AkatukiSora/pjserver-sys#readme",
  "lint-staged": {
    "*": [
      "eslint --fix"
    ],
    ".": [
      "prettier --write ."
    ]
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.92",
    "discord.js": "^14.25.1",
    "dotenv": "^16.6.1",
    "fs-extra": "^11.3.3",
    "log4js": "^6.9.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/eslint": "^9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.19.11",
    "depcheck": "^1.4.7",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "3.5.3",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
  "overrides": {}
}
